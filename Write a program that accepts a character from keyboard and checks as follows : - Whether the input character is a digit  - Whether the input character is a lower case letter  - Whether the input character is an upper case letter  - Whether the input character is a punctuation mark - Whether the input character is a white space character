#include<stdio.h>
#include<conio.h>
main()
{
	char a;
	printf("Enter the character: ");
	scanf("%c,%a");
	if(a>=0&&a<=32)
	printf("\ncharacter can be used as identifier names or not");
	else
	printf("\ncharacter can be used as identifier names");
	if(a>=33&&a<=47)
	printf("\ncharacter is printable");
	else
	printf("\ncharacter is not printable");
	if(a>=48&&a<=57)
	printf("\ncharater is digit");
	else
	printf("\ncharacter is not digit");
	if(a>=58&&a<=64)
	printf("\ncharacter is a white space character");
	else
	printf("\ncharacter is a white space character");
	if(a>=97&&a<=127)
	printf("\ncharacter is lower case");
	else
	printf("\ncharacter is not lower case");
	if(a>=65&&a<=90)
	printf("\ncharcter is upper case");
    else
    printf("\ncharacter is not uppercase");
    
	getch();

}
